# Bread & Butter Development Services
dynamodb: cd /usr/local/dynamodb; java -Djava.library.path=DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb
storage: cd ../open-company-storage; lein start
auth: cd ../open-company-auth; lein start
interaction: cd ../open-company-interaction; lein start
change: cd ../open-company-change; lein start
notify: cd ../open-company-notify; lein start
web: cd ../open-company-web; lein start
